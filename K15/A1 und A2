#A1

class Haus:
    def __init__(self, Straße, Hausnummer, Farbe, Preis, Stockzahl, Quadratmeter) -> None:
        self.Straße = Straße
        self.Hausnummer = Hausnummer
        self.Farbe = Farbe
        self.Preis = Preis 
        self.Stockzahl = Stockzahl
        self.Quadratmeter = Quadratmeter
        
Haus1 = Haus("Mainzer str.", 125, "Blau", 125000, 2, 200)

#A2

class Haus:
    def __init__(self, Straße, Hausnummer, Farbe, Preis, Stockzahl, Quadratmeter) -> None:
        self.Straße = Straße
        self.Hausnummer = Hausnummer
        self.Farbe = Farbe
        self.Preis = Preis 
        self.Stockzahl = Stockzahl
        self.Quadratmeter = Quadratmeter
        
Haus2 = Haus("Mainzer str.", 125, "Blau", 125000, 2, 200)

print(Haus2.__dict__)
